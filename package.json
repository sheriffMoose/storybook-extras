{
    "name": "storybook-extras",
    "version": "0.0.0",
    "license": "MIT",
    "author": {
        "name": "Mostafa Sherif",
        "email": "mosherif87@gmail.com"
    },
    "scripts": {
        "************ DEV **************": "",
        "start": "npm run storybook",
        "storybook": "nx storybook",
        "build:watch": "nx build:watch",
        "build:all:watch": "nx run-many --target=build:watch --all --parallel=10 --exclude=demo",
        "test": "nx test:storybook",
        "************ PROD **************": "",
        "build": "nx build",
        "build:all": "nx run-many --target=build --all --parallel=10 --exclude=demo",
        "build:storybook": "nx build:storybook",
        "chromatic": "nx chromatic",
        "prerelease": "yarn build:all",
        "release": "auto shipit"
    },
    "private": true,
    "devDependencies": {
        "@auto-it/all-contributors": "^10.37.6",
        "@auto-it/conventional-commits": "^10.37.6",
        "@auto-it/first-time-contributor": "^10.37.6",
        "@auto-it/gh-pages": "^10.37.6",
        "@auto-it/npm": "^10.37.6",
        "@auto-it/released": "^10.37.6",
        "@nrwl/nx-cloud": "latest",
        "auto": "^10.3.0",
        "lerna": "^5.1.8",
        "nx": "^15.6.3",
        "prettier": "^2.6.2"
    },
    "workspaces": [
        "demo",
        "packages/*"
    ],
    "auto": {
        "plugins": [
            [
                "gh-pages",
                {
                    "buildCommand": "yarn build:storybook",
                    "dir": "./demo/storybook-static"
                }
            ],
            [
                "npm",
                {
                    "subPackageChangelogs": false
                }
            ],
            "released"
        ]
    },
    "resolutions": {
        "@storybook-extras/angular": "portal:./packages/angular",
        "@storybook-extras/console": "portal:./packages/console",
        "@storybook-extras/devkit": "portal:./packages/devkit",
        "@storybook-extras/markdown": "portal:./packages/markdown",
        "@storybook-extras/swagger": "portal:./packages/swagger",
        "@storybook-extras/toolbars": "portal:./packages/toolbars",
        "@storybook-extras/variants": "portal:./packages/variants"
    },
    "packageManager": "yarn@3.3.1"
}
