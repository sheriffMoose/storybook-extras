{
    "name": "@storybook-extras/devkit",
    "version": "0.0.50",
    "description": "Storybook Extras devkit.",
    "repository": "sheriffMoose/storybook-extras",
    "homepage": "https://github.com/sheriffMoose/storybook-extras/tree/master/packages/devkit#readme",
    "funding": "https://github.com/sheriffMoose/storybook-extras?sponsor=1",
    "license": "MIT",
    "keywords": [
        "storybook",
        "addons",
        "extras",
        "devkit"
    ],
    "author": {
        "name": "Mostafa Sherif",
        "email": "mosherif87@gmail.com"
    },
    "bugs": {
        "url": "https://github.com/sheriffMoose/storybook-extras/issues"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "engines": {
        "node": ">=16"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/ts/index.d.ts",
    "files": [
        "dist",
        "stories",
        "package.json",
        "README.md",
        "CHANGELOG.md",
        "LICENSE",
        "*.js",
        "*.d.ts"
    ],
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./dist/ts/index.d.ts"
        },
        "./array": {
            "import": "./dist/esm/array/index.js",
            "require": "./dist/cjs/array/index.js",
            "types": "./dist/ts/array/index.d.ts"
        },
        "./markdown": {
            "import": "./dist/esm/markdown/index.js",
            "require": "./dist/cjs/markdown/index.js",
            "types": "./dist/ts/markdown/index.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "array": [
                "dist/ts/array/index.d.ts"
            ],
            "markdown": [
                "dist/ts/markdown/index.d.ts"
            ]
        }
    },
    "scripts": {
        "*************** DEV ***************": "",
        "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
        "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
        "build:ts": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
        "*************** BUILD ***************": "",
        "prebuild": "rimraf ./dist",
        "build": "concurrently \"npm run build:cjs\" \"npm run build:esm\" \"npm run build:ts\"",
        "watch": "concurrently \"npm run build:cjs --watch\" \"npm run build:esm --watch\" \"npm run build:ts --watch\""
    },
    "dependencies": {
        "@storybook/csf-tools": "next",
        "@storybook/mdx2-csf": "next",
        "lodash-es": "^4.17.21"
    },
    "devDependencies": {
        "@types/lodash-es": "^4.17.6",
        "concurrently": "^7.6.0",
        "rimraf": "^3.0.2",
        "tslib": "^2.5.0",
        "typescript": "^4.9.4"
    },
    "gitHead": "bc5ab559b9e7fe9c199d19a87d3970ccd0758a03"
}
