{
    "name": "@storybook-extras/toolbars",
    "version": "0.0.38",
    "description": "Storybook addon for adding custom toolbar buttons.",
    "repository": "sheriffMoose/storybook-extras",
    "homepage": "https://github.com/sheriffMoose/storybook-extras/tree/master/packages/toolbars#readme",
    "funding": "https://github.com/sheriffMoose/storybook-extras?sponsor=1",
    "license": "MIT",
    "keywords": [
        "storybook",
        "extras",
        "addons",
        "custom",
        "toolbars",
        "buttons"
    ],
    "author": {
        "name": "Mostafa Sherif",
        "email": "mosherif87@gmail.com"
    },
    "bugs": {
        "url": "https://github.com/sheriffMoose/storybook-extras/issues"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "storybook": {
        "displayName": "Toolbar Buttons",
        "supportedFrameworks": [
            "supported-frameworks"
        ],
        "icon": "https://raw.githubusercontent.com/sheriffMoose/storybook-extras/master/logos/variants.svg"
    },
    "engines": {
        "node": ">=16"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/ts/index.d.ts",
    "files": [
        "dist",
        "main",
        "utils",
        "README.md",
        "*.js",
        "*.d.ts"
    ],
    "scripts": {
        "*************** DEV ***************": "",
        "buildBabel": "concurrently \"yarn buildBabel:cjs\" \"yarn buildBabel:esm\"",
        "buildBabel:cjs": "babel ./src -d ./dist/cjs --extensions \".js,.jsx,.ts,.tsx\"",
        "buildBabel:esm": "babel ./src -d ./dist/esm --env-name esm --extensions \".js,.jsx,.ts,.tsx\"",
        "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
        "*************** BUILD ***************": "",
        "prebuild": "rimraf ./dist",
        "build": "concurrently \"yarn buildBabel\" \"yarn buildTsc\"",
        "build:watch": "concurrently \"yarn buildBabel:esm -- --watch\" \"yarn buildTsc -- --watch\""
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.12.3",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-react": "^7.12.5",
        "@babel/preset-typescript": "^7.13.0",
        "@storybook/addon-essentials": "next",
        "@storybook/angular": "next",
        "@storybook/api": "next",
        "@storybook/components": "next",
        "@storybook/core-events": "next",
        "@storybook/csf-tools": "next",
        "@storybook/react": "next",
        "@storybook/react-webpack5": "next",
        "concurrently": "^7.6.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "storybook": "next",
        "typescript": "4.8.2"
    },
    "peerDependencies": {
        "@storybook/addons": "next",
        "@storybook/api": "next",
        "@storybook/components": "next",
        "@storybook/core-events": "next",
        "@storybook/theming": "next",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "gitHead": "bc5ab559b9e7fe9c199d19a87d3970ccd0758a03"
}
