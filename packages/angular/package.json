{
  "name": "@sheriffmoose/storybook-ngx",
  "version": "1.0.37",
  "description": "Storybook addon to add extra configurations for Angular applications.",
  "repository": "sheriffMoose/storybook-ngx",
  "homepage": "https://sheriffmoose.github.io/storybook-ngx",
  "funding": "https://github.com/sheriffMoose/storybook-ngx?sponsor=1",
  "license": "MIT",
  "keywords": [
    "angular extras",
    "storybook",
    "angular",
    "extras",
    "services",
    "unit testing",
    "docs",
    "compodoc",
    "wrapper",
    "console",
    "coverage",
    "test runner",
    "instrument",
    "webpack",
    "source code",
    "storybook addons",
    "swagger",
    "openapi"
  ],
  "author": {
    "name": "Mostafa Sherif",
    "email": "mosherif87@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/sheriffMoose/storybook-ngx/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "storybook": {
    "displayName": "Angular Extras",
    "supportedFrameworks": [
      "angular"
    ],
    "icon": "https://raw.githubusercontent.com/sheriffMoose/storybook-ngx/main/logo.png"
  },
  "engines": {
    "node": ">=16"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/ts/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "buildBabel": "concurrently \"yarn buildBabel:cjs\" \"yarn buildBabel:esm\"",
    "buildBabel:cjs": "babel ./src -d ./dist/cjs --extensions \".js,.jsx,.ts,.tsx\"",
    "buildBabel:esm": "babel ./src -d ./dist/esm --env-name esm --extensions \".js,.jsx,.ts,.tsx\"",
    "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
    "prebuild": "yarn clean",
    "build": "concurrently \"yarn buildBabel\" \"yarn buildTsc\"",
    "build:watch": "concurrently \"yarn buildBabel:esm -- --watch\" \"yarn buildTsc -- --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "ng run ng-app:start-storybook --port 6006",
    "start": "concurrently \"yarn build:watch\" \"yarn storybook -- --quiet\"",
    "build-storybook": "ng run ng-app:build-storybook",
    "prerelease": "zx scripts/prepublish-checks.mjs",
    "release": "auto shipit",
    "eject-ts": "zx scripts/eject-typescript.mjs",
    "chromatic": "npx chromatic --project-token=d6496f4622fe -d storybook-static --exit-once-uploaded"
  },
  "dependencies": {
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@sheriffmoose/storybook-md": "^1.0.14",
    "swagger-ui-react": "^4.15.5"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1501.2",
    "@angular-devkit/build-angular": "15.1.0",
    "@angular-devkit/core": "15.1.0",
    "@angular/cli": "15.1.2",
    "@angular/common": "15.1.0",
    "@angular/compiler": "15.1.1",
    "@angular/compiler-cli": "15.1.0",
    "@angular/core": "15.1.0",
    "@angular/forms": "15.1.0",
    "@angular/platform-browser": "15.1.0",
    "@angular/platform-browser-dynamic": "15.1.0",
    "@angular/router": "15.1.0",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.13.0",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-a11y": "7.0.0-beta.31",
    "@storybook/addon-docs": "^7.0.0-beta.31",
    "@storybook/addon-essentials": "7.0.0-beta.31",
    "@storybook/addon-interactions": "7.0.0-beta.31",
    "@storybook/addon-links": "7.0.0-beta.31",
    "@storybook/addons": "7.0.0-beta.31",
    "@storybook/angular": "7.0.0-beta.31",
    "@storybook/api": "7.0.0-beta.31",
    "@storybook/components": "7.0.0-beta.31",
    "@storybook/core-events": "7.0.0-beta.31",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/mdx2-csf": "^1.0.0-next.4",
    "@storybook/test-runner": "^0.10.0-next.4",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/theming": "7.0.0-beta.31",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "auto": "^10.3.0",
    "babel-loader": "^8.1.0",
    "boxen": "^5.0.1",
    "chromatic": "^6.14.0",
    "concurrently": "^6.2.0",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-storybook": "^0.6.8",
    "lodash": "^4.17.21",
    "ngx-build-plus": "15.0.0",
    "node-fetch": "^3.3.0",
    "prettier": "^2.8.0",
