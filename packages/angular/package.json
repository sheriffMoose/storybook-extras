    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.1.0",
    "rxjs": "6.5.4",
    "storybook": "7.0.0-beta.31",
    "typescript": "4.8.2",
    "zone.js": "~0.11.4",
    "zx": "^1.14.1"
  },
  "peerDependencies": {
    "@storybook/addons": "7.0.0-beta.31",
    "@storybook/api": "7.0.0-beta.31",
    "@storybook/components": "7.0.0-beta.31",
    "@storybook/core-events": "7.0.0-beta.31",
    "@storybook/theming": "7.0.0-beta.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
