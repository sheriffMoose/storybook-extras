{
  "name": "@sheriffmoose/storybook-ngx",
  "version": "1.0.6",
  "description": "Storybook addon to add extra configurations for Angular applications.",
  "keywords": [
    "storybook",
    "angular",
    "services",
    "unit testing",
    "docs",
    "compodoc",
    "wrapper",
    "console",
    "coverage",
    "test runner",
    "instrument",
    "webpack",
    "source code",
    "storybook addons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sheriffMoose/storybook-ngx"
  },
  "author": "Mostafa Sherif <mosherif87@gmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/ts/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "// postinstall": "node scripts/welcome.js",
    "clean": "rimraf ./dist",
    "buildBabel": "concurrently \"yarn buildBabel:cjs\" \"yarn buildBabel:esm\"",
    "buildBabel:cjs": "babel ./src -d ./dist/cjs --extensions \".js,.jsx,.ts,.tsx\"",
    "buildBabel:esm": "babel ./src -d ./dist/esm --env-name esm --extensions \".js,.jsx,.ts,.tsx\"",
    "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
    "prebuild": "yarn clean",
    "build": "concurrently \"yarn buildBabel\" \"yarn buildTsc\"",
    "build:watch": "concurrently \"yarn buildBabel:esm -- --watch\" \"yarn buildTsc -- --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "ng run ng-app:start-storybook --port 6006",
    "start": "concurrently \"yarn build:watch\" \"yarn storybook -- --quiet\"",
    "build-storybook": "build-storybook",
    "prerelease": "zx scripts/prepublish-checks.mjs",
    "release": "yarn build && auto shipit",
    "eject-ts": "zx scripts/eject-typescript.mjs"
  },
  "dependencies": {
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1500",
    "@angular-devkit/build-angular": "^15",
    "@angular-devkit/core": "^15",
    "@angular/cli": "^15",
    "@angular/common": "^15",
    "@angular/compiler": "^15",
    "@angular/compiler-cli": "^15",
    "@angular/core": "^15",
    "@angular/forms": "^15",
    "@angular/platform-browser": "^15",
    "@angular/platform-browser-dynamic": "^15",
    "@angular/router": "^15",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.13.0",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-a11y": "^7.0.0-beta.14",
    "@storybook/addon-essentials": "^7.0.0-beta.14",
    "@storybook/addon-interactions": "^7.0.0-beta.14",
    "@storybook/addon-links": "^7.0.0-beta.14",
    "@storybook/addons": "^7.0.0-beta.14",
    "@storybook/angular": "^7.0.0-beta.14",
    "@storybook/api": "^7.0.0-beta.14",
    "@storybook/components": "^7.0.0-beta.14",
    "@storybook/core-events": "^7.0.0-beta.14",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/test-runner": "^0.10.0-next.3",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/theming": "^7.0.0-beta.14",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "auto": "^10.3.0",
    "babel-loader": "^8.1.0",
    "boxen": "^5.0.1",
    "concurrently": "^6.2.0",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-storybook": "^0.6.8",
    "ngx-build-plus": "14",
    "node-fetch": "^2.6.1",
    "prettier": "^2.8.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rxjs": "6.5.4",
    "storybook": "^7.0.0-beta.14",
    "typescript": "4.7.3",
    "zone.js": "~0.11.4",
    "zx": "^1.14.1"
  },
  "peerDependencies": {
    "@storybook/addons": "^6.5.8",
    "@storybook/api": "^6.5.8",
    "@storybook/components": "^6.5.8",
    "@storybook/core-events": "^6.5.8",
    "@storybook/theming": "^6.5.8",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "storybook": {
    "displayName": "Angular Extras",
    "supportedFrameworks": [
      "angular"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
