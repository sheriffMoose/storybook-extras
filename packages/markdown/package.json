{
    "name": "@storybook-extras/markdown",
    "version": "0.0.57",
    "description": "Storybook addon for auto importing markdown docs.",
    "repository": "sheriffMoose/storybook-extras",
    "homepage": "https://github.com/sheriffMoose/storybook-extras/tree/master/packages/markdown#readme",
    "funding": "https://github.com/sheriffMoose/storybook-extras?sponsor=1",
    "license": "MIT",
    "keywords": [
        "storybook",
        "addons",
        "extras",
        "docs",
        "markdown",
        "readme",
        "changelog"
    ],
    "author": {
        "name": "Mostafa Sherif",
        "email": "mosherif87@gmail.com"
    },
    "bugs": {
        "url": "https://github.com/sheriffMoose/storybook-extras/issues"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "storybook": {
        "displayName": "Markdown Docs",
        "supportedFrameworks": [
            "supported-frameworks"
        ],
        "icon": "https://raw.githubusercontent.com/sheriffMoose/storybook-extras/master/logos/markdown.svg"
    },
    "engines": {
        "node": ">=16"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/ts/index.d.ts",
    "files": [
        "dist",
        "stories",
        "package.json",
        "README.md",
        "CHANGELOG.md",
        "LICENSE",
        "*.js",
        "*.d.ts"
    ],
    "scripts": {
        "*************** DEV ***************": "",
        "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
        "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
        "build:ts": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
        "*************** BUILD ***************": "",
        "prebuild": "rimraf ./dist",
        "build": "concurrently \"npm run build:cjs\" \"npm run build:esm\" \"npm run build:ts\"",
        "watch": "concurrently \"npm run build:cjs --watch\" \"npm run build:esm --watch\" \"npm run build:ts --watch\""
    },
    "dependencies": {
        "@storybook-extras/devkit": "latest",
        "@storybook/mdx2-csf": "next"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "next",
        "@storybook/angular": "next",
        "@storybook/api": "next",
        "@storybook/components": "next",
        "@storybook/core-events": "next",
        "@storybook/csf-tools": "next",
        "@storybook/react": "next",
        "@storybook/react-webpack5": "next",
        "concurrently": "^7.6.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "storybook": "next",
        "tslib": "^2.5.0",
        "typescript": "4.9.5"
    },
    "peerDependencies": {
        "@storybook/addons": "next",
        "@storybook/api": "next",
        "@storybook/components": "next",
        "@storybook/core-events": "next",
        "@storybook/theming": "next",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "gitHead": "bc5ab559b9e7fe9c199d19a87d3970ccd0758a03"
}
